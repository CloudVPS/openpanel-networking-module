<xml.schema>

	<xml.class name="openpanel.module.domain">
		<xml.type>dict</xml.type>
		<xml.proplist>
			<xml.member class="config"	id="config"/>
		</xml.proplist>
	</xml.class>

	<xml.class name="config">
		<xml.type>dict</xml.type>
		<xml.proplist>
			<xml.member class="varpath" 			id="varpath"/>
			<xml.member class="system:interfaces" 	id="system:interfaces"/>
			<xml.member class="system:network"		id="system:network"/>
			<xml.member class="system:hostname"		id="system:hostname"/>
			<xml.member class="system:networkdir"	id="system:networkdir"/>
			<xml.member class="system:servicename"	id="system:servicename"/>
		</xml.proplist>
	</xml.class>

	<xml.class name="system:networkdir">
		<xml.type>string</xml.type>
	</xml.class>

	<xml.class name="system:servicename">
		<xml.type>string</xml.type>
	</xml.class>

	<xml.class name="varpath">
		<xml.type>string</xml.type>
	</xml.class>

	<xml.class name="system:etc">
		<xml.type>string</xml.type>
	</xml.class>

	<xml.class name="system:network">
		<xml.type>string</xml.type>
	</xml.class>

	<xml.class name="system:hostname">
		<xml.type>string</xml.type>
	</xml.class>

	
</xml.schema>